import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        String[] partyNames = {"Oskars", "Anna", "Andris"};

        System.out.println("Enter the name for the party: ");

        Scanner scanner = new Scanner(System.in);
        String partyGoers = scanner.nextLine();
        scanner.close();

        boolean isFound = false;

        for (int i = 0; i < partyNames.length; i++) {
            if (partyNames[i].equals(partyGoers)) {
                isFound = true;
                break;
            }
        }

        if (isFound) {
            System.out.println("You are invited to the party!");
        } else {
            System.out.println("Sorry, no entrance");
        }
    }
}
